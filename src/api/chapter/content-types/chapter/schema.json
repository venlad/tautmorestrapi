{
  "kind": "collectionType",
  "collectionName": "chapters",
  "info": {
    "singularName": "chapter",
    "pluralName": "chapters",
    "displayName": "Chapters",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "chapters"
    },
    "topic": {
      "displayName": "topic",
      "type": "component",
      "repeatable": true,
      "component": "topic.topic"
    },
    "grade": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::grade.grade",
      "inversedBy": "chapters"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "chapterNumber": {
      "type": "string",
      "required": true
    },
    "co_corricular": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::co-corricular.co-corricular",
      "inversedBy": "chapters"
    },
    "isCoCorricular": {
      "type": "enumeration",
      "enum": [
        "subject",
        "coCorricular"
      ],
      "default": "subject",
      "required": true
    }
  }
}
